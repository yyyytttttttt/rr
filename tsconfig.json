{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/lib/auth.js", "src/app/api/upload-avatar/route.js", "src/app/components/profile/UploadClient.jsx", "src/app/api/resend/route.ts", "src/app/profile/admin/page.jsx", "src/app/Servic/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
